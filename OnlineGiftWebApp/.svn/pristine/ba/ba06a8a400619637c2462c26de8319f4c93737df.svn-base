  }
  .slider-icon-contain a:hover{
    color: $white !important;
    text-decoration: none;
  }
  .slider-icon-height-change img{
    height: 45px ;
  }
  .slider-icon-height img{
     height: 35px;
  }
  %row-custom{
    margin-right: -15px;
    margin-left: -15px;
  }
  .row-custom{
      @extend %row-custom;
  }
  .slider-side-shape{
    height: 100%;
    padding-left: 15px;
    background-color: transparent;
    position: relative;
    z-index: 10;
    min-height: 81vh;
  }
  .slider-side-shape:after{
    content: '';
    z-index: -1;
    position: absolute;
    width: 100%;
    height: 100%;
    top: 0;
    right: 0;
    background-image: url(../assets/images/shape.png);
    background-size: 100% 100%;
    opacity: 0.5;
  }
  .slide-play-icon{
    height: 100px;
    width: 100px;
    display: inline-block;
    background-color: transparent;
    border:10px solid rgba(255,255,255,0.5);
    color: $white;
    line-height: 108px;
    text-align: center;
    border-radius: 50%;
    position: relative;
    z-index: 16;
    cursor: pointer;
    transition: all 0.5s;
    box-shadow: 0 0 0 rgba(204,169,44, 0.5);
    animation: custom-pulse 2s infinite;
    i{
        font-size: 37px;
    }
  }
  .slide-play-icon-width{
    width: 100px;
  }
  .slide-play-icon:hover{
    background-color:$green;
  }
  .slide-play-icon:after{
    content: '';
    top:0;
    right: 0;
    position: absolute;
    z-index: 10;
    height: 100%;
    width: 100%;
    background-color: transparent;
    border:2px solid $dark;
    border-radius: 50%;
  
  }
  
  @-webkit-keyframes custom-pulse {
    0% {
      -webkit-box-shadow: 0 0 0 0 rgba(255,255,255, 0.4);
    }
    70% {
        -webkit-box-shadow: 0 0 0 10px rgba(255,255,255, 0);
    }
    100% {
        -webkit-box-shadow: 0 0 0 0 rgba(255,255,255, 0);
    }
  }
  @keyframes custom-pulse {
    0% {
      -moz-box-shadow: 0 0 0 0 rgba(255,169,255, 0.4);
      box-shadow: 0 0 0 0 rgba(255,255,255, 0.4);
    }
    70% {
        -moz-box-shadow: 0 0 0 10px rgba(255,169,255, 0);
        box-shadow: 0 0 0 10px rgba(255,255,255, 0);
    }
    100% {
        -moz-box-shadow: 0 0 0 0 rgba(255,255,255, 0);
        box-shadow: 0 0 0 0 rgba(255,169,255, 0);
    }
  }
  $text-shadow :  0px 2px 10px rgba(0,0,0,0.5);
  .slide-play-icon-tag-line{
    font-size: 30px;
    font-weight: 400;
    color: $white;
    width: calc(100% - 100px);
  }
  .slide-play-icon-tag-line span{
    font-family: $great-vibes;
    color: $dark;
    font-size: 35px;
    text-shadow: $text-shadow;
  }
  .navbar-toggler{
      display: none;
  }
  .min-width-40px{
      min-width: 40px !important;
  }

