  padding-right: 0 !important;
  width: 34px;
  border-radius: 50% !important;
}
.prise-range-button{
  min-width: inherit !important;
  width: 100% !important;
}
%one-line-ellips{
  white-space: nowrap; 
  overflow: hidden;
  -o-text-overflow: ellipsis;
     text-overflow: ellipsis;
}
.one-line-ellips{
  @extend %one-line-ellips;
}
.border-color-light-gray{
  border-color: $light-gray !important;     
}
$gift-card-img : 210px;
$card-box-shadow :0 3px 1px -2px rgba(0,0,0,.2), 0 2px 2px 0 rgba(0,0,0,.14), 0 1px 5px 0 rgba(0,0,0,.12);
.gift-card{
  margin-bottom: 0.7rem;
  
  .gift-card-img{
      height: $gift-card-img;
      overflow: hidden;
      box-shadow: $card-box-shadow;
      border-radius: 7px;
      margin-bottom: 1rem;
    img{
      transition: all 2s;
      min-width: 100%;
      height: $gift-card-img;
    }
  }
  .gift-card-contain{
    h6{
      @extend %one-line-ellips;
      margin-bottom: 0.3rem;
    }
    .gift-card-prise{
      margin-bottom: 0.4rem;
      span{
        font-family: $great-vibes;
        color: $green;
        font-size: 30px;
      }
    }
  }
  &:hover{
    .gift-card-img{
      img{
        transform: scale(1.3);
      }
    }
  }
}
$send-gift-img-height : 133px;
.send-gift-img{
  height: $send-gift-img-height;
  overflow: hidden;
  border-radius: 5px;
  box-shadow: $card-box-shadow;
  img{
    height: $send-gift-img-height;
    min-width: 100%;
  }
}
.mygift-profile-bg{
  background-image: url(../assets/images/image28.jpg);
  position: relative;
  background-size: cover;
  background-position: center;
  box-shadow: $card-box-shadow;
  z-index: 5;
}
.mygift-profile-bg:after{
  content: '';
  position: absolute;
  top: 0;
  right: 0;
  z-index: -1;
  height: 100%;
  width: 100%;
  background-color: $white;
  opacity: 0.8;
}

/*myaccount*/
$myaccount-profile-width : 25%;
$myaccount-cover-img : 320px;
.myaccount-profile-cover{
  height: $myaccount-cover-img;
  overflow: hidden;
  box-shadow: $card-box-shadow;
  border-radius: 5px;
  position: relative;
  z-index: 10;
  img{
    min-height: $myaccount-cover-img;
    width: 100%;
  }
  .myaccount-profile-cover-btn{
    position: absolute;
    top: 10px;
    right: 10px;
    z-index: 15;
    opacity: 0;
    top: -20px;
    @extend %transistion-5s;
    .mat-button{
      color: $white;
      font-size: 16px;
      background-color: rgba(0,0,0,0.4);
      @extend %transistion-5s;
    }
  }
  &:hover{
    .myaccount-profile-cover-btn{
      top: 10px;
      opacity: 1;
    }
  }
  &:after{
    content: '';
    position: absolute;
    height: 100%;
    width: 100%;
    background: rgb(255,255,255);
    background: linear-gradient(183deg, rgba(255,255,255,0) 0%, rgba(0,0,0,0.9051995798319328) 100%);
    opacity: 0.8;
    z-index: 12;
    top: 0;
    right: 0;
  }
  .myaccount-profile-name{
    position: absolute;
    left: $myaccount-profile-width;
    bottom: 10px;
    color: $white;
    padding-left: 1rem;
    z-index: 20;
    h5{
      color: $white;
      font-size: 25px;
      font-weight: 300;
    }
  }
}
.w-80{
  width: 80%;
}
.w-95{
  width: 95%;
}
$myaccount-profile-main-image-height : 200px;
.myaccount-profile-main-image{
  margin-top: -6rem;
  position: relative;
  z-index: 30;  
  width:$myaccount-profile-width;
  
  
  .myaccount-profile-main-image-height{
    margin-left: 23%;
      height: $myaccount-profile-main-image-height;
      overflow: hidden;
      border-radius: 4px;
      box-shadow: $card-box-shadow;
      img{
        height: $myaccount-profile-main-image-height;
        min-width: 100%;
      }
  }
}
.edit-profile-link{
    padding-left: 0;
    width: 100%;
    li{
      list-style: none;
      a{
        width: 100%;
        color: $muted;
        font-size: 16px;
        padding: 0.7rem 0.5rem;
        display: inline-block;
        border: 1px solid $light-gray;
        margin-top: -1px;
        background-color: $white;
        @extend %transistion-5s;
        text-align: left;
        i{
          font-size: 22px;
          padding-right: 0.5rem;
        }
        span{
          display: inline-block;

        }
        &.active,
        &:focus,
        &:hover{
          border-left: 2px solid $green;
          background-color: $lighter-white;          
          color: $dark;
        }
      }
    }
  
}
.myorder-card{
  margin-bottom: 15px;
  .myorder-id{
    background-color: $lighter-white;
    padding:8px 15px;
  }
  .myorder-contain{
    border: 1px solid $light-gray;
    padding: 15px;
  }
}
$myorder-gift-img : 40px;
.myorder-contain{
  .myorder-gift-img{
    height: $myorder-gift-img;
    width: $myorder-gift-img;
    border-radius: 50%;
    overflow: hidden;
    display: inline-block;
    margin-right: 10px;
    margin-left: 10px;
    img{
      height: $myorder-gift-img;
      min-width: 100%;
    }
  }
}
.mygift-order-status{
  display: inline-block;
  color: $white;
  padding: 2px 3px;
  font-size: 13px;
  border-radius: 2px;
  font-weight: 300;
}
.bg-green{
  background-color: #2fca71;
}
.bg-red{
  background-color: #ec4f42;
}
.bg-yellow{
  background-color: #e8c31e;
}
