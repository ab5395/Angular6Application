.menu-logo{
    height: 70px;
    transition: all 0.5s;
  }
  
  %px-10px{
    padding-right:10px  !important;
    padding-left: 10px !important;
  }
  .px-10px{
      @extend %px-10px;
  }
  .main-menu {
    li {
        a{
    color: $white;
    position: relative;
    display: inline-block;
    @extend %px-15px;
    &:hover{
        text-decoration: none;
    }
    &:after{
        content: ']';
        position: absolute;
        right: 10px;
        top: 0px;
        @extend %transistion-5s;
        color: $green;
        opacity: 0;
        }
    &:before{
        content: '[';
        position: absolute;
        left: 10px;
        top: 0px;
        @extend %transistion-5s;
        color: $green;
        opacity: 0;
        }
        }
    }
  }
  
  .main-menu li > a:hover:before,.main-menu li > a:focus:before,.main-menu li > a.active:before{
    left: 2px;
    opacity: 1;
  }
  .main-menu li > a:hover:after,.main-menu li > a:focus:after,.main-menu li > a.active:after{
    right: 2px;
    opacity: 1;
  }
  
  .menu-toogle-btn{
    background-color: rgba(255,255,255,0.2) !important;
    color: $green ;
    border-radius: 0px !important;
  }
  .fixed-menu{
    position: fixed;
    top: 0;
    right: 0;
    width: 100%;
    z-index: 999;
    @extend %transistion-5s;
    background-color: transparent;
  }
  .fixed-menu{
      min-height: $fixed-menu-size;
  }
